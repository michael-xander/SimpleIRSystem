[ test2.qasm |test2.png |test2.pdf |test2.eps |test2.tex ]

    
    
    # 
    # File:   test2.qasm
    # Date:   29-Mar-04
    # Author: I. Chuang 
    #
    # Sample qasm input file - simple teleportation circuit
    #
            qubit 	q0
            qubit 	q1
    	qubit 	q2
    
    	h	q1	# create EPR pair
    	cnot	q1,q2
    	cnot	q0,q1	# Bell basis measurement
    	h	q0
    	nop	q1
    	measure	q0	
    	measure	q1
    	c-x	q1,q2	# correction step
    	c-z	q0,q2
      
  
